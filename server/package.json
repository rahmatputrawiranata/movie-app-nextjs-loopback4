{
  "name": "movie-apps",
  "version": "0.0.1",
  "description": "movie app",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "18 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t movie-apps .",
    "docker:run": "docker run -p 3000:3000 -d movie-apps",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build",
    "dev": "nodemon -r source-map-support/register ."
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^11.0.1",
    "@loopback/authentication-jwt": "^0.15.1",
    "@loopback/boot": "^7.0.1",
    "@loopback/core": "^6.0.1",
    "@loopback/repository": "^7.0.1",
    "@loopback/rest": "^14.0.1",
    "@loopback/rest-explorer": "^7.0.1",
    "@loopback/service-proxy": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "loopback-connector-openapi": "^6.2.0",
    "loopback-connector-rest": "^5.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.1",
    "@loopback/eslint-config": "^15.0.1",
    "@loopback/testlab": "^7.0.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^16.18.95",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "ignore": [
      "dist/*"
    ],
    "ext": "ts",
    "exec": "yarn start"
  }
}
